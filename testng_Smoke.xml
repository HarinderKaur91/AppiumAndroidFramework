<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<groups>
		<run>
			<include name="Smoke"/>
		</run>
	</groups>

	<listeners>
		<listener class-name="com.Harinder.Utils.Listeners" />
	</listeners>


	<test thread-count="5" name="End to end Framework Test">
		<classes>
			<class name="com.Harinder.Tests.AndroidStoreTest" />
		</classes>
	</test> <!-- Test -->

	<test thread-count="5" name="Error Validation Test">
		<classes>
			<class name="com.Harinder.Tests.ErrorValidationTest" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
